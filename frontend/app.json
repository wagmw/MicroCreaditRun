      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundImage": "./assets/icon.png",
        "backgroundColor": "#ffbf00"
      },
      "package": "com.nayana4u.microcreditrun"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "5bb0c63e-0c68-48b5-a22e-0fe929cd7bf9"
      }
    }
  }
}
