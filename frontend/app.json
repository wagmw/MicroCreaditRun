{
  "expo": {
    "name": "Sinha Investment",
    "slug": "microcredit-run",
    "scheme": "microcreditrun",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
